{
  "entrypoint": "index.html",
  "shell": "src/chup-app.html",
  "fragments": [
    "src/views/promo-view.html",
    "src/views/confirmation-view.html",
    "src/views/items-view.html",
    "src/views/inventory-view.html",
    "src/views/location-view.html",
    "src/views/notfound-view.html",
    "src/views/order-view.html",
    "src/views/start-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    }
  ]
}
